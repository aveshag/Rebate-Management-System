{
  "version": 1,
  "disable_existing_loggers": false,
  "loggers": {
    "": {
      "level": "DEBUG",
      "handlers": [
        "console",
        "file"
      ]
    }
  },
  "formatters": {
    "default": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "default"
    },
    "file": {
      "class": "logging.FileHandler",
      "filename": "app.log",
      "formatter": "default"
    }
  }
}